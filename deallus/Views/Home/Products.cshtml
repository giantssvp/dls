
@{
    ViewBag.Title = "Products";
    Layout = "~/Views/Shared/_Gst_Layout.cshtml";
}

<div id="content-header">
    <div id="breadcrumb"> <a href='@Url.Action("Dashboard", "Home")' title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a> <a href="#">Add Products</a></div>
</div>
<div class="container-fluid">
    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-title">
                    <span class="icon"> <i class="icon-info-sign"></i> </span>
                    <h5>Add Product Details</h5>
                </div>
                <div class="widget-content nopadding">
                    <form method="post" action="#" name="basic_validate" id="basic_validate" novalidate="novalidate">
                        <table class="table table-striped">
                            <tbody>
                                <tr class="odd gradeX">
                                    <td><label class="control-label">Product Code</label></td>
                                    <td><input type="text" name="pcode" id="required"></td>
                                    <td><label class="control-label">Product Name</label></td>
                                    <td> <input type="text" name="pname" id="required"></td>
                                    <td><label class="control-label">HSN</label></td>
                                    <td><input type="text" name="phsn" id="required"></td>
                                </tr>
                                <tr class="odd gradeX">
                                    <td><label class="control-label">CGST</label></td>
                                    <td><input type="text" name="pcgst" id="required"></td>
                                    <td><label class="control-label">SGST</label></td>
                                    <td> <input type="text" name="psgst" id="required"></td>
                                    <td><label class="control-label">IGST</label></td>
                                    <td><input type="text" name="pigst" id="required"></td>
                                </tr>
                                <tr class="odd gradeX">
                                    <td><label class="control-label">Rate Per Piece</label></td>
                                    <td><input type="text" name="prate" id="required"></td>
                                    <td><label class="control-label">Product Description</label></td>
                                    <td> <textarea class="span11"></textarea></td>
                                    <td></td>
                                    <td><input type="submit" value="Validate" class="btn btn-success"></td>
                                </tr>
                            </tbody>
                        </table>
                    </form>
                </div>
            </div>
            <div class="widget-box">
                <div class="widget-title">
                    <span class="icon"><i class="icon-th"></i></span>
                    <h5>All Products</h5>
                </div>
                <div class="widget-content nopadding">
                    <table class="table table-bordered data-table">
                        <thead>
                            <tr>
                                <th>Rendering engine</th>
                                <th>Browser</th>
                                <th>Platform(s)</th>
                                <th>Engine version</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="gradeX">
                                <td>Trident</td>
                                <td>
                                    Internet
                                    Explorer 4.0
                                </td>
                                <td>Win 95+</td>
                                <td class="center">4</td>
                            </tr>
                            <tr class="gradeC">
                                <td>Trident</td>
                                <td>
                                    Internet
                                    Explorer 5.0
                                </td>
                                <td>Win 95+</td>
                                <td class="center">5</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Trident</td>
                                <td>
                                    Internet
                                    Explorer 5.5
                                </td>
                                <td>Win 95+</td>
                                <td class="center">5.5</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Trident</td>
                                <td>
                                    Internet
                                    Explorer 6
                                </td>
                                <td>Win 98+</td>
                                <td class="center">6</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Trident</td>
                                <td>Internet Explorer 7</td>
                                <td>Win XP SP2+</td>
                                <td class="center">7</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Trident</td>
                                <td>AOL browser (AOL desktop)</td>
                                <td>Win XP</td>
                                <td class="center">6</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Gecko</td>
                                <td>Firefox 1.0</td>
                                <td>Win 98+ / OSX.2+</td>
                                <td class="center">1.7</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Gecko</td>
                                <td>Firefox 1.5</td>
                                <td>Win 98+ / OSX.2+</td>
                                <td class="center">1.8</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Gecko</td>
                                <td>Firefox 2.0</td>
                                <td>Win 98+ / OSX.2+</td>
                                <td class="center">1.8</td>
                            </tr>
                            <tr class="gradeA">
                                <td>Gecko</td>
                                <td>Firefox 3.0</td>
                                <td>Win 2k+ / OSX.3+</td>
                                <td class="center">1.9</td>
                            </tr>
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>    
</div>

<script src="~/Scripts/gst_js/jquery.min.js"></script>
<script src="~/Scripts/gst_js/jquery.ui.custom.js"></script>
<script src="~/Scripts/gst_js/bootstrap.min.js"></script>
<script src="~/Scripts/gst_js/jquery.uniform.js"></script>
<script src="~/Scripts/gst_js/select2.min.js"></script>
<script src="~/Scripts/gst_js/jquery.validate.js"></script>

<script src="~/Scripts/gst_js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/gst_js/matrix.js"></script>
<script src="~/Scripts/gst_js/matrix.tables.js"></script>

<script src="~/Scripts/gst_js/matrix.form_validation.js"></script>